{
    "name": "docslab/ipay88",
    "type": "library",
    "description": "iPay88 Online Payment Switching Gateway PHP library.",
    "keywords": ["iPay88", "Payment Gateway"],
    "homepage": "https://github.com/DocsLab/php-ipay88",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Maxime Gilbert (DocBu)",
            "email": "docbu@docslab.net",
            "homepage": "https://github.com/DocBu",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "https://github.com/DocsLab/php-ipay88",
        "issues": "https://github.com/DocsLab/php-ipay88/issues",
        "source": "https://github.com/DocsLab/php-ipay88/tree/master",
        "wiki": "https://github.com/DocsLab/php-ipay88/wiki"
    },
    "require": {
        "php" : "^5.6|^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/http-foundation" : "^3.0|^4.0",
        "symfony/validator" : "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0"
    },
    "suggest": {
        "symfony/event-dispatcher": "Allows other libraries to alter the requests to and the responses from iPay88 Online Payment Switching Gateway and to handle exceptions thrown by this library.",
        "symfony/validator": "Allows to validate the requests to and the responses from iPay88 Online Payment Switching Gateway."
    },
    "autoload": {
        "psr-4": {
            "DocsLab\\IPay88\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
